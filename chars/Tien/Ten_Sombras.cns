ground.hittime  = 10             ;Time opponent is in hit state
ground.velocity = -3.5             ;Velocity at which opponent is pushed
airguard.velocity = -1.9,-.8     ;Guard velocity in air (def: (air.xvel*1.5, air.yvel/2))

;Y-velocity at which opponent is pushed
air.hittime = 12                 ;Time before opponent regains control in air


[State Dest]
type = destroyself
;trigger1 = Movecontact = 1
trigger1 = Time = 11               ; Acá debería ir el tiempo que dura el frame que golpea

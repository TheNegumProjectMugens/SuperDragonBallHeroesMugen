sprite   = graphical\18SFF.sff    ;Sprite
anim     = coding\18AIR.air       ;Animation
sound    = audio\18SND.snd        ;Sound
pal1     = graphical\18.act       ;Standard Android 18
pal2     = graphical\Yuuna.act    ;Yuuna Miyama(Maburaho)
pal3     = graphical\Bulma_N.act  ;Namek Saga Bulma
pal4     = graphical\18b.act      ;Android 18 Alternate Colors
pal5     = graphical\Tifa.act     ;Tifa Lockheart(Final Fantasy VII)
pal6     = graphical\18c.act      ;Android 18 in black
pal7     = graphical\Rin.act      ;Rin Kamishiro(Maburaho)
pal8     = graphical\Aerith.act   ;Aerith Gainsborough(Final Fantasy VII)
pal9     = graphical\Bulma_A.act  ;Android Saga Bulma
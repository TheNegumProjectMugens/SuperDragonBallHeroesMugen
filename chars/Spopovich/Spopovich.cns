attr = S, NA
animtype  = Medium
damage    = 57
guardflag = MA
pausetime = 12,12
sparkno = s7001
sparkxy = -10,-70
hitsound   = s811,0
guardsound = 6,0
ground.type = High
ground.slidetime = 12
ground.hittime  = 13

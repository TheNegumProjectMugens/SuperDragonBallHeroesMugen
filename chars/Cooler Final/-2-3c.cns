;-----------------------------------------------------------------------------
[StateDef -3]

;--------------------------------------------------------------------------------------------------

[State -3, 変数の初期化]
type = Null
triggerall = !IsHelper
triggerall = !RoundState
trigger1   = var(56) := 0 || 1	;親変更可能領域フラグ
ignorehitpause = 1

;--------------------------------------------------------------------------------------------------
;  親変更ヘルパーのIDを調べ, 親変更可能かどうかを調べる。
;  マクベス氏のリーゼの記述を参考にさせて頂きました。

[State -3, 親変更ヘルパーID調査]
type = VarSet
triggerall = !IsHelper
triggerall = TeamSide = 1		;1P側限定
triggerall = NumHelper(16000) && NumHelper(16001) && NumHelper(16002)
triggerall = (Helper(16000),ID % 256 = [30,60])
triggerall = (Helper(16001),ID % 256 = [30,60])
trigger1   = (Helper(16002),ID % 256 = [30,60])
var(56) = 1	;親変更可能領域フラグを立てる
ignorehitpause = 1

;--------------------------------------------------------------------------------------------------
